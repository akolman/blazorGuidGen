<Project Sdk="Microsoft.NET.Sdk.BlazorWebAssembly">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <Authors>Andrew Kolman</Authors>
    <Company />
    <Product>Blazor GUID Generator</Product>
    <Description>Client-side (wasm) batch GUID generator</Description>
    <PackageProjectUrl>https://guid.akolman.info</PackageProjectUrl>
    <RepositoryUrl>https://gitlab.com/Akolman/blazorGUIDgen</RepositoryUrl>
    <Nullable>enable</Nullable>
    <AssemblyVersion>1.2.1.0</AssemblyVersion>
    <FileVersion>1.2.1.0</FileVersion>
    <Version>1.2.1</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly" Version="6.0.0-preview.4.21253.5" />
    <PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly.DevServer" Version="6.0.0-preview.4.21253.5" PrivateAssets="all" />
  </ItemGroup>

</Project>
